    "react-snap": "^1.23.0",
    "rollup-plugin-svelte": "^7.1.0",
    "stylelint": "^13.11.0",
    "stylelint-config-standard": "^20.0.0",
    "svelte": "^3.32.3",
    "svelte-preprocess": "^4.6.9",
