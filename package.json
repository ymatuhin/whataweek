{
  "name": "whataweek",
  "version": "1.0.4",
  "scripts": {
    "dev": "vite",
    "build": "vite build --outDir dist",
    "prerender": "react-snap",
    "preview": "npm run build && npm run prerender && npm run serve",
    "serve": "vite preview",
    "deploy": "git subtree push --prefix dist origin gh-pages",
    "prepare": "husky install"
  },
  "reactSnap": {
    "source": "dist",
    "skipThirdPartyRequests": true,
    "inlineCss": true,
    "minifyCss": true
  },
  "browserslist": [
    "defaults"
  ],
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.6",
    "@tailwindcss/jit": "0.1.8",
    "@tsconfig/svelte": "^1.0.10",
    "@types/node": "^14.14.41",
    "@vitejs/plugin-legacy": "^1.3.2",
    "@ymatuhin/debug": "^0.0.10",
    "autoprefixer": "^10.2.5",
    "nano-css": "^5.3.1",
    "postcss": "^8.2.10",
    "postcss-load-config": "^3.0.1",
    "react-snap": "^1.23.0",
    "svelte": "^3.37.0",
    "svelte-preprocess": "^4.7.0",
    "tailwindcss": "^2.1.1",
    "typescript": "^4.2.4",
    "vite": "^2.1.5",
    "husky": "^6.0.0"
  },
  "dependencies": {
    "shared": "file:./shared"
  }
}
